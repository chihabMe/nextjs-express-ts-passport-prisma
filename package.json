    "test": "pnpm db:deploy:test && pnpm server:test",
    "dev": "pnpm dotenv -e .env.development -- pnpm nodemon --delay 1.5 --watch server ./server/app.ts ",
    "dev:https": "pnpm dotenv -e .env.development -- pnpm nodemon --delay 1.5 --watch server ./server/app.ts https ",
    "start": "NODE_ENV=production node ./dist/app.js",
    "start:test": "NODE_ENV=production pnpm dotenv -e .env.production -- node ./dist/app.js",
    "web:build": "pnpm next build",
    "server:build": "tsc   --project tsconfig.server.json",
    "build": "pnpm web:build && pnpm server:build",
    "lint": "next lint",
    "db:migrate": "pnpm dotenv -e .env.development -- pnpm prisma migrate dev",
    "db:deploy": "pnpm dotenv -e .env.development -- pnpm prisma migrate deploy",
    "db:generate": "pnpm dotenv -e .env.development -- pnpm prisma generate",
    "db:studio:dev": "pnpm dotenv -e .env.development -- pnpm prisma studio"
